        </shape>
    </item>
    <item>
        <inset android:inset="20dp">
            <shape android:shape="oval">
                <stroke
                    android:width="20dp"
                    android:color="#5F5995" />
            </shape>
        </inset>
    </item>
    <item>
        <inset android:inset="20dp">
            <shape android:shape="oval">
                <stroke
